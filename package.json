{
  "name": "I-Invest",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "APP_VARIANT=development npx expo start",
    "start:preview": "NODE_ENV=staging APP_VARIANT=preview npx expo start",
    "android": "APP_VARIANT=development npx expo run:android",
    "android:preview": "NODE_ENV=staging APP_VARIANT=preview npx expo run:android",
    "ios": "APP_VARIANT=development npx expo run:ios",
    "ios:preview": "NODE_ENV=staging APP_VARIANT=preview npx expo run:ios",
    "web": "APP_VARIANT=development npx expo start --web",
    "test": "jest --watchAll",
    "check-types": "NODE_OPTIONS=\"--max_old_space_size=3072 --max-semi-space-size=32\" tsc --noEmit",
    "prettier:fix": "npx prettier --write '**/*.{js,jsx,ts,tsx,json}'",
    "lint": "npx eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "npx eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "fix:all": "yarn run prettier:fix && yarn run lint:fix",
    "prepare": "husky"
  },
  "lint-staged": {
    "**/*.{ts,tsx,js,jsx}": "npx eslint --fix"
  },
  "jest": {
    "preset": "jest-expo"
  },
